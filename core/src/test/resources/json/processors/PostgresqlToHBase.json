{
	"id": "Processor-PostgresqlToHBase",
	"source": {
		"logical.name": "parrot_source",
		"source.class": "io.parrot.processor.source.postgresql.PostgreSqlSource",
		"bootstrap.servers": "kafka:9092",
		"table.whitelist": "parrot.table_with_simple_pk"
	},
	"sink": {
		"id": "Sink-HBase",
		"sink.class": "io.parrot.processor.sink.hbase.HBaseSink",
		"configurations": {
			"hbase.zookeeper.quorum": "zookeeper:2181",
			"hive.external.enabled": "true",
			"hive.external.port": "10000",
			"hive.external.database": "parrot",
			"hive.external.host": "hive",
			"hive.username": "parrot",
			"hive.password": "parrot"
		}
	}
}